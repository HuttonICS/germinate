<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:b='urn:import:org.gwtbootstrap3.client.ui'>

	<b:ButtonGroup>
		<b:Button ui:field="firstPage" icon="ANGLE_DOUBLE_LEFT"/>
		<b:Button ui:field="prevPage" icon="ANGLE_LEFT"/>
		<b:Button ui:field="pagerText" enabled="false"/>
		<b:Button ui:field="nextPage" icon="ANGLE_RIGHT"/>
		<b:Button ui:field="lastPage" icon="ANGLE_DOUBLE_RIGHT"/>
	</b:ButtonGroup>
</ui:UiBinder> 