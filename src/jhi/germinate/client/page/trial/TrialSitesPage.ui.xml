<ui:UiBinder xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'
			 xmlns:ui='urn:ui:com.google.gwt.uibinder'>

	<ui:with field="msg" type="jhi.germinate.client.i18n.Text"/>

	<g:HTMLPanel>
		<b:PageHeader text="{msg.trialSitesTitle}" ui:field="header"/>
		<b:Heading size="H4" text="{msg.notificationNoDataFound}" visible="false" ui:field="noDataHeader"/>

		<g:FlowPanel visible="false" ui:field="detailsPanel">

		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>